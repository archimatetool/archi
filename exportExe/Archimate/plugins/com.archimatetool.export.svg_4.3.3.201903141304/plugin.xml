<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.archimatetool.editor.imageExportProvider">
      <imageExportProvider
            class="com.archimatetool.export.svg.SVGExportProvider"
            extensions="svg"
            id="com.archimatetool.export.svg.imageExporter"
            label="SVG Image">
      </imageExportProvider>
      <imageExportProvider
            class="com.archimatetool.export.svg.PDFExportProvider"
            extensions="pdf"
            id="com.archimatetool.export.pdf.imageExporter"
            label="PDF Image">
      </imageExportProvider>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.archimatetool.export.svg.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
